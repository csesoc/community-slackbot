{
	"type": "modal",
	"callback_id": "anonymous_messaging_modal",
	"title": {
		"type": "plain_text",
		"text": "Anonymous Messaging",
		"emoji": true
	},
	"submit": {
		"type": "plain_text",
		"text": "Send",
		"emoji": true
	},
	"close": {
